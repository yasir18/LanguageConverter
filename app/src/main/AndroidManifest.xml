<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yasir.languageconverter">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/hindi"
        android:label="EngToHin"
        android:roundIcon="@mipmap/hindi"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Numbers"
           android:label="Numbers"/>

        <activity android:name=".FamilyMembers"
            android:label="Family Members"/>
        <activity android:name=".phrases"
            android:label="Phrases"/>

        <activity android:name=".colors"
            android:label="Colors"/>


    </application>

</manifest>